Module Module1

    Sub Main()
        'Finding Vowels In A String

        Dim str1 As String
        Dim i, vowelscounter As Integer
        Dim char1 As Char

        str1 = ""
        char1 = ""
        i = 0
        vowelscounter = 0

        Console.Write("Please Enter String Which Is To Be Processed: ")
        str1 = Console.ReadLine


        For i = 1 To Len(str1)
            char1 = Mid(str1, i, 1)
            char1 = UCase(char1)
            If char1 = "A" Or char1 = "E" Or char1 = "I" Or char1 = "O" Or char1 = "U" Then
                vowelscounter = vowelscounter + 1

            End If


        Next i

        Console.WriteLine("Vowels In String Are: " & vowelscounter)


        Console.ReadKey()
    End Sub

End Module
