Module Module1

    Sub Main()
        'Inversing A String
        Dim str1, str2 As String
        Dim char1 As Char
        Dim i As Integer

        str1 = ""
        str2 = ""
        char1 = ""
        i = 0

        Console.Write("Please Enter String Which Needs to be processed: ")
        str1 = Console.ReadLine

        For i = Len(str1) To 1 Step -1
            char1 = Mid(str1, i, 1)
            str2 = str2 + char1



        Next i
        Console.Write("Processed String Is: " & str2)
        Console.ReadKey()

    End Sub

End Module
